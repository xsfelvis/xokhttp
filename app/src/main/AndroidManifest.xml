<?xml version="1.0" encoding="utf-8"?>
<manifest package="xsf.xokhttp"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name="xsf.xokhttp.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="xsf.xokhttp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="xsf.xokhttp.MVCActivity">
        </activity>
        <activity android:name="xsf.xokhttp.MVPActivity">
        </activity>
    </application>

</manifest>
